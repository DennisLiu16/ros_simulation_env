<launch>	
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">	
     <param name="base_global_planner" value="navfn/NavfnROS" />
<!-- 	<param name="base_global_planner" value="global_planner/GlobalPlanner" /> -->
    <param name="planner_frequency"  value="2" />
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />

    <rosparam file="$(find nav_move_base)/launch/params/global_costmap_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find nav_move_base)/launch/params/local_costmap_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find nav_move_base)/launch/params/dwa_base_local_planner_params.yaml" command="load" />
<!--   <remap from = "cmd_vel" to= "move_base_cmd_vel"/>  -->
  </node>
  <node pkg="nav_move_base" type="Kidnapping.py" name="Kidnap" output="screen" />
</launch>
